{
  "name": "aws-saml-login",
  "version": "0.1.1",
  "description": "A simple cli utility to get temporary AWS credentials via a SAML endpoint",
  "main": "./dist/index.js",
  "bin": {
    "aws-saml-login": "./dist/index.js"
  },
  "scripts": {
    "watch": "tsc -w",
    "start": "ts-node src/index.ts",
    "debug": "node --inspect-brk -r ts-node/register ./src/index.ts",
    "build": "tsc -p .",
    "clean": "rm -rf dist/"
  },
  "author": "Matt Aschmann",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattaschmann/aws-saml-login"
  },
  "license": "ISC",
  "engines": {
    "node": ">=10.12.0"
  },
  "dependencies": {
    "aws-sdk": "2.476.0",
    "colors": "1.3.3",
    "commander": "2.20.0",
    "ini": "1.3.5",
    "puppeteer": "1.17.0",
    "readline-sync": "1.4.9"
  },
  "devDependencies": {
    "@types/ini": "1.3.30",
    "@types/puppeteer-core": "1.9.0",
    "@types/readline-sync": "1.4.3",
    "ts-node": "8.3.0",
    "tslint": "5.17.0",
    "typescript": "3.5.1"
  }
}
